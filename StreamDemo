package com.ss.streamAPI;

import java.util.Arrays;
import java.util.List;

public class StreamDemo {

	public static void main(String[] args) {
		
		List<Integer> list =Arrays.asList(25,45,35,20,32);
		
		/*Stream<Integer> stream =list.stream();
		
		Stream<Integer> filter=stream.filter(i->i>30);
		
		filter.forEach(i->System.out.println(i));*/
		
		list.stream().filter(i->i>30).forEach(i->System.out.println(i));
         
	}

}
